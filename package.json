    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "postcss": "^8.5.6",
    "typescript": "~5.8.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx,js,jsx}",
      "components/**/*.{ts,tsx,js,jsx}",
      "services/**/*.{ts,tsx,js,jsx}",
      "!**/*.d.ts",
      "!**/index.{ts,tsx,js}",
      "!**/__tests__/**"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*))"
